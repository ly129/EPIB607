# Normal probability calculations

```{r, fig.keep='none'}
## ---- Question-1 ------------------------------------------------------------
#(a)
#mosaic::xpnorm(q = -1.80, mean = 0, sd = 1)
prob_less_1.80 <- mosaic::xpnorm(q = -1.80, mean = 0, sd = 1)
prob_less_1.80 <- round(prob_less_1.80, 2)
#(b)
 #mosaic::xpnorm(q = -1.80, mean = 0, sd = 1, lower.tail = FALSE)
prob_greater_1.80 <- mosaic::xpnorm(q = -1.80, mean = 0, sd = 1, lower.tail = FALSE)
prob_greater_1.80 <- round(prob_greater_1.80, 2)
#(c)
#mosaic::xpnorm(q = 1.60, mean = 0, sd = 1, lower.tail = FALSE)
prob_greater_1.60 <- mosaic::xpnorm(q = 1.60, mean = 0, sd = 1, lower.tail = FALSE)
prob_greater_1.60 <- round(prob_greater_1.60, 2)
#(d)
#mosaic::xpnorm(q = 1.60, mean =0, sd=1) - mosaic:: xpnorm(q = -1.80, mean =0, sd=1)
prob_d <- mosaic::xpnorm(q = 1.60, mean =0, sd=1) - mosaic:: xpnorm(q = -1.80, mean =0, sd=1)
prob_d <- round(prob_d, 2)
```

1(A).

